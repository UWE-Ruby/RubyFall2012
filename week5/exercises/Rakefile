task :test do
	puts "Hello World!"
end

task :names do
  getNamesFromFile.each do |name|
      puts name
  end
end

task :createclassdir do
  Dir.mkdir(:RubyClass) unless Dir.exists?(:RubyClass)
end

task :createnamedir => [:createclassdir] do

end

def getNamesFromFile(file="names")
  store_names = []
  f = File.open("names", "r")
  f.each do |name|
    store_names << name
  end
  f.close
  return store_names
end
